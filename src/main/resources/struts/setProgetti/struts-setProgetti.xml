<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
	"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
     
    <include file="struts/setProgetti/struts-progetto.xml" />
     
    
    
    <package name="setProgetti" extends="default" namespace="/">

		<action name="elencoSetProgetti"  class="elencoSetProgettiAction" >
			<result name="success" >/jsp/setProgetti/elencoSetProgetti.jsp</result>
		</action>
			
		<action name="setProgetti"  class="setProgettiAction" >
			<result name="success" >/jsp/setProgetti/setProgetti.jsp</result>
			<result name="input" >/jsp/setProgetti/setProgetti.jsp</result>
			<result name="elencoSetProgetti" type="redirectAction">elencoSetProgetti</result>
		</action>
		
		
	</package>
        
</struts>
